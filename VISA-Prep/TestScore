def can_achieve_marks(N, X, Y):
    # Check if Y is a multiple of X and Y is within the achievable range
    if Y % X == 0 and Y <= N * X:
        return "YES"
    else:
        return "NO"

# Reading the input
N, X, Y = map(int, input().split())

# Output the result
print(can_achieve_marks(N, X, Y))
